{
  "version": 2,
  "builds": [
    {
      "src": "k2so_engine.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/k2so-talk",
      "dest": "k2so_engine.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}